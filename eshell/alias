alias erc call-interactively #'erc
alias ed comint ed $*
alias pacman yay --repo $*
alias d dired $*
alias l ls -lah $*
alias ,p ls -lh $*
alias S systemctl $*
alias SS sudo systemctl $*
alias SU systemctl --user $*
alias f (find-file $*)
alias push (let ((display-buffer-alist (cons '("\\ *git push\\*" display-buffer-no-window) display-buffer-alist))) (async-shell-command "git push" " *git push*"))
